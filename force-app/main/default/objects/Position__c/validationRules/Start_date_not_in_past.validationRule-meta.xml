<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_date_not_in_past</fullName>
    <active>true</active>
    <errorConditionFormula>Start_date__c &lt;= TODAY()</errorConditionFormula>
    <errorDisplayField>Start_date__c</errorDisplayField>
    <errorMessage>Start date must be in the future.</errorMessage>
</ValidationRule>
